#include<iostream>
using namespace std;
int main()
{
	string str;
	cout<<"Enter the string in zero or one"<<endl;
	cin>>str;
	int n=str.length();
	if(!str[0]=='0'||str[0]=='1')
	{
		cout<<"Only enter 0 or 1"<<endl;
		cout<<"Invalid string Entered"<<endl;
	
	}
	else
	{
		if(str[n-1]=='0')
		cout<<"It is an even binary number"<<endl;
		else
		cout<<"It is an Odd binary number"<<endl;
		
	}
}
