	cout<<"Enter any string: ";
	cin>>str;
	
	//n= str.length();
	int flag=0;
	for(int i=0;i<n;i++)
	{
		if (str[i]=='0')
		{
			one_comp[i]='1';
		}
		else if (str[i]=='1')
		{
			one_comp[i]='0';
			
		}
		//str.replace(1,0)
		//str.replace(0,1)
			
		}
		one_comp[n]='0';
		
		
	}
	




